@model SchoolProject.Models.Form.Programs;
@using SchoolProject.Models.Data;
<link rel="stylesheet" href="~/css/addstud.css" />
<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="#">Admin Page</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item active">
        <a class="nav-link" href="/School/Admin">Home <span class="sr-only">(current)</span></a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/School/AddStudent">Student</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#">Professor</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/School/AddProgram">Degree</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/School/AddAdmin">Admin</a>
      </li>s
    </ul>
  </div>
</nav>

<h3>Admin Users</h3>
<table class="table table-sm table-striped">
  <thead>
    <tr>
      <th>Admin ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Type</th>
      <th>Username</th>
    </tr>
  </thead>
  <tbody>
     @{
      @foreach (var item in @ViewBag.admin as IEnumerable<Admin_User>)
      {
        <tr>
          <td>@item.admin_id</td>
          <td>@item.firstname</td>
          <td>@item.lastname</td>
          <td>@item.type</td>
          <td>@item.username</td>
        </tr>
      }
    }
  </tbody>
</table>

<h3>Programs</h3>
<table class="table table-sm table-striped">
  <thead>
    <tr>
      <th>Program ID</th>
      <th>Program Code</th>
      <th>Program Title</th>
      <th>Major</th>
    </tr>
  </thead>
  <tbody>
     @{
      @foreach (var item in @ViewBag.programs as IEnumerable<Programs>)
      {
        <tr>
          <td>@item.program_id</td>
          <td>@item.program_code</td>
          <td>@item.program_title</td>
          <td>@item.major</td>
        </tr>
      }
    }
  </tbody>
</table>